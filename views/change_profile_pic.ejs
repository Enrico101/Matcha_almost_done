<body>
    <header>Matcha!</header>
    <nav>
        <a href="/profile">profile</a>
        <a href="/home">Home</a>
        <a href="/search">Search</a>
        <a href="/chat_screen">Chat</a>
        <a href="/view_history">view_history</a>
        <a href="/visit_history">visit_history</a>
        <a href="/like_history">like history</a>
        <a href="/profile_settings">Profile Setting</a>
        <a href="/logout">logout</a>
    </nav>
    <div>
        <%
            let x = 0;
            if (images_edit != "")
            {
                while (images_edit[x])
                {
        %>
                    <form action="/profile_settings/change_profile_pic" method="post">
                        <img src=<%=images_edit[x].image%> style="width: 400px; height: 300px;">
                        <br />
                        <input type="hidden" name="set_this_pic" value=<%=images_stock[x].image%>>
                        <input type="submit" name="submit" value="set">
                        <br />
                    </form>
        <%
                    x++;
                }
            }
        %>
    </div>
    <footer>2019</footer>
</body>
</html>